<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/evaluation_navigation"
	app:startDestination="@id/superHeroListFragment">
	<fragment
		android:id="@+id/superHeroListFragment"
		android:name="org.shujito.evaluation.SuperHeroListFragment"
		android:label="SuperHeroListFragment"
		tools:layout="@layout/super_hero_list_fragment">
		<action
			android:id="@+id/action_start_to_detail"
			app:destination="@id/superHeroDetailFragment"
			app:enterAnim="@anim/nav_default_enter_anim"
			app:exitAnim="@anim/nav_default_exit_anim"
			app:popEnterAnim="@anim/nav_default_pop_enter_anim"
			app:popExitAnim="@anim/nav_default_pop_exit_anim" />
	</fragment>
	<fragment
		android:id="@+id/superHeroDetailFragment"
		android:name="org.shujito.evaluation.SuperHeroDetailFragment"
		android:label="SuperHeroDetailFragment"
		tools:layout="@layout/super_hero_detail_fragment" />
</navigation>
